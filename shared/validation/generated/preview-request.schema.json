{
  "type": "object",
  "properties": {
    "imageUrl": {
      "type": "string",
      "minLength": 1
    },
    "style": {
      "type": "string",
      "minLength": 1
    },
    "photoId": {
      "type": "string"
    },
    "aspectRatio": {
      "type": "string",
      "pattern": "^\\d+:\\d+$",
      "default": "1:1"
    },
    "watermark": {
      "type": "boolean",
      "default": true
    },
    "quality": {
      "type": "string",
      "enum": [
        "low",
        "medium",
        "high",
        "auto"
      ],
      "default": "medium"
    },
    "cacheBypass": {
      "type": "boolean",
      "default": false
    },
    "isAuthenticated": {
      "type": "boolean"
    },
    "sourceStoragePath": {
      "type": [
        "string",
        "null"
      ]
    },
    "sourceDisplayUrl": {
      "type": [
        "string",
        "null"
      ]
    },
    "cropConfig": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "x": {
              "type": "number"
            },
            "y": {
              "type": "number"
            },
            "width": {
              "type": "number"
            },
            "height": {
              "type": "number"
            },
            "orientation": {
              "type": "string"
            }
          },
          "additionalProperties": {}
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "imageUrl",
    "style"
  ],
  "additionalProperties": true,
  "$id": "previewRequest"
}
