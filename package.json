{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:registry && vite build",
    "build:registry": "tsx scripts/build-style-registry.ts",
    "build:analyze": "npm run build:registry && VERBOSE_SOURCEMAPS=true ANALYZE=1 vite build",
    "build:dev": "npm run build:registry && vite build --mode development",
    "thumbnails:generate": "tsx scripts/generate-thumbnail-formats.ts",
    "validate:registry": "npm run build:registry && git diff --quiet -- src/config/styles/styleRegistry.generated.ts supabase/functions/_shared/styleRegistry.generated.ts || (echo \"Style registry output is outdated. Run 'npm run build:registry' and commit the generated files.\" && exit 1)",
    "lint": "eslint .",
    "lint:unused": "eslint . --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "dead-code:check": "npm run dead-code:exports && npm run dead-code:files && npm run dead-code:deps",
    "dead-code:exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=src/main.tsx,src/App.tsx,src/pages",
    "build:preview-validators": "tsx scripts/generate-preview-validators.ts",
    "dead-code:files": "unimported --flow --init --update",
    "dead-code:deps": "unimported --show-unresolved",
    "dead-code:fix": "npm run lint:unused && npm run dead-code:clean",
    "dead-code:clean": "unimported --remove-unused",
    "deps:analyze": "madge --circular --extensions ts,tsx src/",
    "deps:check": "depcheck",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@stripe/react-stripe-js": "^2.6.2",
    "@stripe/stripe-js": "^3.1.0",
    "@supabase/auth-js": "^2.65.1",
    "@tanstack/react-query": "^5.56.2",
    "canvas-confetti": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.12",
    "heic2any": "^0.0.4",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.4.2",
    "react-router-dom": "^6.26.2",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.20",
    "depcheck": "^1.4.7",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.3",
    "lovable-tagger": "^1.1.7",
    "madge": "^8.0.0",
    "postcss": "^8.4.47",
    "react-test-renderer": "^18.3.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.11",
    "ts-unused-exports": "^10.1.0",
    "tsx": "^4.11.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.0.1",
    "unimported": "^1.31.1",
    "vite": "^5.4.1",
    "vitest": "^1.6.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
