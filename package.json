    "husky": "^9.0.11",
    "lovable-tagger": "^1.1.7",
    "madge": "^8.0.0",
    "postcss": "^8.4.47",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.11",
    "ts-unused-exports": "^10.1.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.0.1",
    "unimported": "^1.31.1",
    "vite": "^5.4.1"
